{
  "name": "lead-gen-my-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspaces --if-present",
    "dev:web": "npm run dev --workspace apps/web",
    "dev:worker": "npm run dev --workspace apps/worker",
    "build": "npm run build --workspaces --if-present",
    "lint": "npm run lint --workspaces --if-present",
    "db:migrate": "npm run migrate --workspace packages/db",
    "db:seed": "npm run seed --workspace packages/db",
    "db:studio": "npm run studio --workspace packages/db"
  },
  "devDependencies": {
    "dotenv-cli": "^11.0.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.2",
    "turbo": "latest",
    "typescript": "^5.3.3"
  }
}
